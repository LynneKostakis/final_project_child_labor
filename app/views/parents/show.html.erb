<div class="page-header">
  <h3>Parent #<%= @parent.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Parent name</dt>
      <dd><%= @parent.parent_name %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/parents" class="btn btn-primary">
        Back
      </a>
      <a href="/parents/<%= @parent.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_parent/<%= @parent.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
