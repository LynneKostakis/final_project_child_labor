<div class="page-header">
  <h1>
    Families

  </h1>


      
      
<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Family name</th>

        <th>Actions</th>
      </tr>
      
      <% @families.each do |family| %>
      <tr>
        <td><%= family.family_name %></td>
        <td>
          
       <div class="panel-footer">          
        
          <a href="/families/<%= family.id %>" class="btn btn-primary">Show</a>
          <a href="/families/<%= family.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_family/<%= family.id %>" class="btn btn-danger" rel="nofollow">Delete</a>

         </div>
        <% end %>
        </td>
      </tr>
      
      
     
    </table>
    <%= paginate @families, theme: 'twitter-bootstrap-4' %>
  </div>
</div>



